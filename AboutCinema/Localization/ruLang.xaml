<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:AboutCinema.Localization"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">
    <!--Login Page-->
    <v:String x:Key="_LogP_LoginOrRegTitle">Войдите или зарегистрируйтесь</v:String>
    <v:String x:Key="_LogP_Email">Email:</v:String>
    <v:String x:Key="_LogP_Password">Пароль:</v:String>
    <v:String x:Key="_LogP_Enter">Войти</v:String>
    <v:String x:Key="_LogP_Register">Зарегистрироваться</v:String>

    <!--Register Page-->
    <v:String x:Key="_RegP_RegTitle">Регистрация</v:String>
    <v:String x:Key="_RegP_Nick">Никнейм:</v:String>
    <v:String x:Key="_RegP_Email">Email:</v:String>
    <v:String x:Key="_RegP_Password">Пароль:</v:String>
    <v:String x:Key="_RegP_RepPassword">Повторите пароль:</v:String>
    <v:String x:Key="_RegP_Back">Назад</v:String>
    <v:String x:Key="_RegP_Register">Зарегистрироваться</v:String>



</ResourceDictionary>